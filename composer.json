{
    "name": "demvsystems/php-wsdl",
    "description": "php wsdl",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/demvsystems/php-wsdl",
    "keywords": [
        "WSDL",
        "Wsdl",
        "Xsd"
    ],
    "authors": [
        {
            "name": "Randy Sch√ºtt",
            "email": "rswhite4@gmail.com"
        }
    ],
    "require": {
        "ext-dom": "*",
        "php": "^8.1",
        "dgame/php-ensurance": "^2.3",
        "demvsystems/php-soap": "^2.0",
        "guzzlehttp/guzzle": "^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:demvsystems/php-soap.git"
        }
    ],
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Dgame\\Wsdl\\": "src/",
            "Dgame\\Wsdl\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ],
        "coverage": [
            "phpunit --coverage-clover=coverage"
        ]
    }
}
